{
    "name": "ofat/laravel-translatable-routes",
    "description": "Laravel package for translatable routes",
    "license": "MIT",
    "keywords": [
        "laravel",
        "translatable"
    ],
    "authors": [
        {
            "name": "Vitalii Ofat",
            "email": "ofatv22@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "files": ["src/helpers.php"],
        "psr-4": {
            "Ofat\\LaravelTranslatableRoutes\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ofat\\LaravelTranslatableRoutes\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ofat\\LaravelTranslatableRoutes\\TranslatableRoutesServiceProvider"
            ]
        }
    },
    "require-dev": {
        "nunomaduro/phpinsights": "^2.2"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
